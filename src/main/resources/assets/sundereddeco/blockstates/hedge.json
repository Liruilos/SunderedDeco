{
  "multipart": [
    {   "when": { "top": "false" },
      "apply": { "model": "sundereddeco:block/garden/hedge_bottom_single"}},
    {   "when": { "north": "true" },
      "apply": { "model": "sundereddeco:block/garden/hedge_bottom_submodel", "y": 270 }
    },
    {   "when": { "east": "true" },
      "apply": { "model": "sundereddeco:block/garden/hedge_bottom_submodel"}
    },
    {   "when": { "south": "true" },
      "apply": { "model": "sundereddeco:block/garden/hedge_bottom_submodel", "y": 90 }
    },
    {   "when": { "west": "true" },
      "apply": { "model": "sundereddeco:block/garden/hedge_bottom_submodel", "y": 180 }
    },
    {   "when": { "top": "true" },
      "apply": { "model": "sundereddeco:block/garden/hedge_top_single"}},
    {   "when": { "topnorth": "true" },
      "apply": { "model": "sundereddeco:block/garden/hedge_top_submodel", "y": 270}
    },
    {   "when": { "topeast": "true" },
      "apply": { "model": "sundereddeco:block/garden/hedge_top_submodel"}
    },
    {   "when": { "topsouth": "true" },
      "apply": { "model": "sundereddeco:block/garden/hedge_top_submodel", "y": 90 }
    },
    {   "when": { "topwest": "true" },
      "apply": { "model": "sundereddeco:block/garden/hedge_top_submodel", "y": 180 }
    }
  ]
}
